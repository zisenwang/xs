[
  {
    "source": "/public/images/(.*)\\.(png|jpg|jpeg|webp)$",
    "headers": [
      {
        "key": "Cache-Control",
        "value": "public,max-age=31536000,immutable"
      }
    ]
  },
  {
    "source": "/public/video/(.*)\\.(mp4|webm|ogg)$",
    "headers": [
      {
        "key": "Cache-Control",
        "value": "public,max-age=31536000,immutable"
      }
    ]
  }
]
